  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT CASE WHEN DD.AF = 1 AND DD.VT = 1 AND DD.PSVT = 1 THEN 'Severe Heart Rate Issues' WHEN DD.AF = 1 AND DD.VT = 1 THEN 'Abnormal Heart Rate Isssues' WHEN DD.AF = 1 AND DD.PSVT = 1 THEN 'Abnormal Heart Rate Isssues' WHEN DD.VT = 1 AND 
             DD.PSVT = 1 THEN 'Abnormal Heart Rate Isssues' WHEN DD.AF = 1 THEN 'Light Heart Rate Issues' WHEN DD.VT = 1 THEN 'Light Heart Rate Issues' WHEN DD.PSVT = 1 THEN 'Light Heart Rate Issues' ELSE 'No Heart Rate Issues' END AS Disease, COUNT(*) AS PatientCount, 
             DimPerson.GENDER, DimDate.Year
FROM   FactDiagnosisReport AS FDR INNER JOIN
             DimDiagnosis AS DD ON FDR.DiagnosisID = DD.DiagnosisID INNER JOIN
             DimPerson ON FDR.PersonID = DimPerson.PersonID INNER JOIN
             DimDate ON FDR.AdmissionDateID = DimDate.DateKey AND FDR.DischargeDateID = DimDate.DateKey
GROUP BY CASE WHEN DD.AF = 1 AND DD.VT = 1 AND DD.PSVT = 1 THEN 'Severe Heart Rate Issues' WHEN DD.AF = 1 AND DD.VT = 1 THEN 'Abnormal Heart Rate Isssues' WHEN DD.AF = 1 AND DD.PSVT = 1 THEN 'Abnormal Heart Rate Isssues' WHEN DD.VT = 1 AND 
             DD.PSVT = 1 THEN 'Abnormal Heart Rate Isssues' WHEN DD.AF = 1 THEN 'Light Heart Rate Issues' WHEN DD.VT = 1 THEN 'Light Heart Rate Issues' WHEN DD.PSVT = 1 THEN 'Light Heart Rate Issues' ELSE 'No Heart Rate Issues' END, DimPerson.GENDER, DimDate.Year
ORDER BY PatientCount DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Disease">
          <DataField>Disease</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PatientCount">
          <DataField>PatientCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="GENDER">
          <DataField>GENDER</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
