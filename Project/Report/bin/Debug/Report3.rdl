  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
    CASE 
        WHEN DD.AF = 1 THEN 'Atrial Fibrillation'
        WHEN DD.VT = 1 THEN 'Ventricular Tachycardia'
        WHEN DD.PSVT = 1 THEN 'Paroxysmal Supra Ventricular Tachycardia'
        ELSE 'No Heart Rate Issues'
    END AS Disease,
    COUNT(*) AS PatientCount
FROM 
    FactDiagnosisReport FDR
    JOIN DimDiagnosis DD ON FDR.DiagnosisID = DD.DiagnosisID
GROUP BY 
    CASE 
        WHEN DD.AF = 1 THEN 'Atrial Fibrillation'
        WHEN DD.VT = 1 THEN 'Ventricular Tachycardia'
        WHEN DD.PSVT = 1 THEN 'Paroxysmal Supra Ventricular Tachycardia'
        ELSE 'No Heart Rate Issues'
    END
ORDER BY 
    PatientCount DESC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Disease">
          <DataField>Disease</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PatientCount">
          <DataField>PatientCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
