    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>-- Tính số lượng bệnh nhân cho từng bệnh riêng biệt và hợp nhất kết quả
SELECT
    Disease,
    Gender,
    Rural,
    COUNT(*) AS PatientCount,
    AVG(TLC) AS Avg_TLC,
    AVG(PLATELETS) AS Avg_PLATELETS,
    AVG(UREA) AS Avg_UREA,
    AVG(GLUCOSE) AS Avg_GLUCOSE
FROM (
    -- Bệnh CKD
    SELECT
        'CKD' AS Disease,
        DP.GENDER AS Gender,
        DP.RURAL AS Rural,
        DPH.TLC,
        DPH.PLATELETS,
        DPH.UREA,
        DPH.GLUCOSE
    FROM
        FactAdmissionReport FAR
        JOIN DimAnaemia DA ON FAR.AnaemiaID = DA.AnaemiaID
        JOIN DimPerson DP ON FAR.PersonID = DP.PersonID
        JOIN DimPersonHealth DPH ON FAR.PersonHealthID = DPH.PersonHealthID
    WHERE
        DA.CKD = 1
    UNION ALL
    -- Bệnh Anaemia
    SELECT
        'Anaemia' AS Disease,
        DP.GENDER AS Gender,
        DP.RURAL AS Rural,
        DPH.TLC,
        DPH.PLATELETS,
        DPH.UREA,
        DPH.GLUCOSE
    FROM
        FactAdmissionReport FAR
        JOIN DimAnaemia DA ON FAR.AnaemiaID = DA.AnaemiaID
        JOIN DimPerson DP ON FAR.PersonID = DP.PersonID
        JOIN DimPersonHealth DPH ON FAR.PersonHealthID = DPH.PersonHealthID
    WHERE
        DA.ANAEMIA = 1
    UNION ALL
    -- Bệnh Severe Anaemia
    SELECT
        'Severe Anaemia' AS Disease,
        DP.GENDER AS Gender,
        DP.RURAL AS Rural,
        DPH.TLC,
        DPH.PLATELETS,
        DPH.UREA,
        DPH.GLUCOSE
    FROM
        FactAdmissionReport FAR
        JOIN DimAnaemia DA ON FAR.AnaemiaID = DA.AnaemiaID
        JOIN DimPerson DP ON FAR.PersonID = DP.PersonID
        JOIN DimPersonHealth DPH ON FAR.PersonHealthID = DPH.PersonHealthID
    WHERE
        DA.[SEVERE ANAEMIA] = 1
    UNION ALL
    -- Bệnh Diabetes Mellitus (DM)
    SELECT
        'Diabetes Mellitus' AS Disease,
        DP.GENDER AS Gender,
        DP.RURAL AS Rural,
        DPH.TLC,
        DPH.PLATELETS,
        DPH.UREA,
        DPH.GLUCOSE
    FROM
        FactAdmissionReport FAR
        JOIN DimAnaemia DA ON FAR.AnaemiaID = DA.AnaemiaID
        JOIN DimPerson DP ON FAR.PersonID = DP.PersonID
        JOIN DimPersonHealth DPH ON FAR.PersonHealthID = DPH.PersonHealthID
    WHERE
        DA.DM = 1
    UNION ALL
    -- Bệnh Atrial Fibrillation
    SELECT
        'Atrial Fibrillation' AS Disease,
        DP.GENDER AS Gender,
        DP.RURAL AS Rural,
        DPH.TLC,
        DPH.PLATELETS,
        DPH.UREA,
        DPH.GLUCOSE
    FROM
        FactDiagnosisReport FDR
        JOIN DimDiagnosis DD ON FDR.DiagnosisID = DD.DiagnosisID
        JOIN DimPerson DP ON FDR.PersonID = DP.PersonID
        JOIN DimPersonHealth DPH ON FDR.PersonHealthID = DPH.PersonHealthID
    WHERE
        DD.AF = 1
    UNION ALL
    -- Bệnh Ventricular Tachycardia
    SELECT
        'Ventricular Tachycardia' AS Disease,
        DP.GENDER AS Gender,
        DP.RURAL AS Rural,
        DPH.TLC,
        DPH.PLATELETS,
        DPH.UREA,
        DPH.GLUCOSE
    FROM
        FactDiagnosisReport FDR
        JOIN DimDiagnosis DD ON FDR.DiagnosisID = DD.DiagnosisID
        JOIN DimPerson DP ON FDR.PersonID = DP.PersonID
        JOIN DimPersonHealth DPH ON FDR.PersonHealthID = DPH.PersonHealthID
    WHERE
        DD.VT = 1
    UNION ALL
    -- Bệnh Paroxysmal Supra Ventricular Tachycardia
    SELECT
        'Paroxysmal Supra Ventricular Tachycardia' AS Disease,
        DP.GENDER AS Gender,
        DP.RURAL AS Rural,
        DPH.TLC,
        DPH.PLATELETS,
        DPH.UREA,
        DPH.GLUCOSE
    FROM
        FactDiagnosisReport FDR
        JOIN DimDiagnosis DD ON FDR.DiagnosisID = DD.DiagnosisID
        JOIN DimPerson DP ON FDR.PersonID = DP.PersonID
        JOIN DimPersonHealth DPH ON FDR.PersonHealthID = DPH.PersonHealthID
    WHERE
        DD.PSVT = 1
    UNION ALL
    -- Bệnh Infective Endocarditis
    SELECT
        'Infective Endocarditis' AS Disease,
        DP.GENDER AS Gender,
        DP.RURAL AS Rural,
        DPH.TLC,
        DPH.PLATELETS,
        DPH.UREA,
        DPH.GLUCOSE
    FROM
        FactDiagnosisReport FDR
        JOIN DimDiagnosis DD ON FDR.DiagnosisID = DD.DiagnosisID
        JOIN DimPerson DP ON FDR.PersonID = DP.PersonID
        JOIN DimPersonHealth DPH ON FDR.PersonHealthID = DPH.PersonHealthID
    WHERE
        DD.[INFECTIVE ENDOCARDITIS] = 1
    UNION ALL
    -- Bệnh Chest Infection
    SELECT
        'Chest Infection' AS Disease,
        DP.GENDER AS Gender,
        DP.RURAL AS Rural,
        DPH.TLC,
        DPH.PLATELETS,
        DPH.UREA,
        DPH.GLUCOSE
    FROM
        FactDiagnosisReport FDR
        JOIN DimDiagnosis DD ON FDR.DiagnosisID = DD.DiagnosisID
        JOIN DimPerson DP ON FDR.PersonID = DP.PersonID
        JOIN DimPersonHealth DPH ON FDR.PersonHealthID = DPH.PersonHealthID
    WHERE
        DD.[CHEST INFECTION] = 1
) AS SubQuery
GROUP BY 
    Disease,
    Gender,
    Rural
ORDER BY 
    PatientCount DESC;</CommandText>
